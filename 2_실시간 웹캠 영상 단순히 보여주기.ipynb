   "source": [
    "import cv2\n",
    "cap = cv2.VideoCapture(0)\n",
    "while cap.isOpened() == True:\n",
    "    success, image = cap.read()\n",
    "    if success == False:\n",
    "        continue\n",
    "\n",
    "    cv2.imshow('webcam_window01', image)\n",
    "    key = cv2.waitKey(5)\n",
    "    if key == ord('q'):\n",
    "        break\n",
    "cap.release()\n",
    "cv2.destroyAllWindows()"
   ]
  }
